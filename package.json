{
  "name": "armazem-sao-joaquim",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:netlify": "next build",
    "build:static": "next build && node scripts/copy-static-assets.js",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-icons": "node scripts/generate-icons.js",
